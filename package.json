{
  "name": "djs-tickets",
  "version": "5.2.1",
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "description": "An easy to setup ticket system discord bot for Djs v12/v13, works for unlimited servers",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "build": "tsc",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "prepare" : "npm run build",
    "version" : "git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "keywords": [
    "discord ticket",
    "discord",
    "discord.js",
    "moderation",
    "discord_ticket",
    "discord ticket system",
    "discord_ticket_system",
    "discord ticket",
    "discord js ticket",
    "djs ticket",
    "djs ticket system",
    "discord system ticket",
    "ticket systen",
    "ticket discord",
    "discord",
    "ticket"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgamerx/djs-tickets.git"
  },
  "author": "PGamerX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgamerx/djs-tickets/issues"
  },
  "homepage": "https://djs-tickets.js.org",
  "dependencies": {
    "discord.js": "^13.2.0",
    "enmap": "^5.8.7"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  }
}
